{
  "name": "localball",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build-debug": "detox build --configuration ios.sim.debug",
    "test:e2e": "detox test --configuration ios.sim.debug --loglevel info",
    "phone": "react-native run-ios --device \"Eric's iPhone\"",
    "lint": "eslint --fix --ext .js,.jsx .",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.0.2",
    "@react-native-community/cli": "4.13.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.8.0",
    "@react-native-community/push-notification-ios": "^1.5.0",
    "@react-native-firebase/app": "8.4.2",
    "@react-native-firebase/auth": "9.2.2",
    "@react-native-firebase/crashlytics": "8.4.3",
    "@react-native-firebase/firestore": "7.8.1",
    "@react-native-firebase/functions": "7.4.3",
    "@react-native-firebase/messaging": "7.8.6",
    "@react-navigation/bottom-tabs": "^5.9.0",
    "@react-navigation/core": "^5.12.4",
    "@react-navigation/drawer": "^5.9.1",
    "@react-navigation/native": "^5.7.4",
    "@react-navigation/stack": "^5.9.1",
    "firebase-admin": "9.2.0",
    "geolib": "3.2.1",
    "prop-types": "^15.6.2",
    "react": "16.13.1",
    "react-native": "^0.63.2",
    "react-native-elements": "2.3.2",
    "react-native-fbsdk": "^2.0.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-maps": "0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.11.0",
    "react-native-vector-icons": "6.6.0",
    "react-redux": "7.1.3",
    "redux": "4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "0.0.7",
    "@testing-library/react-native": "^7.0.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "detox": "^17.5.8",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^24.8.0",
    "metro": "^0.58.0",
    "metro-core": "^0.58.0",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.1.2"
  },
  "jest": {
    "preset": "react-native",
    "notify": true
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "jest/__e2e__/config.json",
    "specs": "e2e",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/LocalBall.app",
        "build": "react-native run-ios",
        "type": "ios.simulator",
        "name": "iPhone 11"
      }
    }
  }
}
